datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../../../../node_modules/.prisma/client"
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String
  password String
}

model Book {
  id         String @id @default(uuid())
  title      String
  author     String
  coverImage String
}
